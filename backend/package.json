{
  "name": "visa-service-backend",
  "version": "1.0.0",
  "description": "Backend API for Visa Requirements Viewer",
  "engines": {
    "node": ">=18.0.0"
  },
  "main": "index.js",
  "scripts": {
    "prestart": "nvm use",
    "start": "node index.js",
    "dev": "nvm use && nodemon index.js",
    "setup": "nvm use && node utils/csvToJson.js",
    "restart": "nvm use && pkill -f 'node.*index.js' && sleep 1 && node index.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "fs": "^0.0.1-security",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "visa",
    "api",
    "express",
    "nodejs"
  ],
  "author": "Unikwik Team",
  "license": "MIT"
} 